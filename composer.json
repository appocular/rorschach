{
    "name": "xendk/rorschach",
    "description": "Do visual tests using Selenium and Appocular.",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Thomas Fini Hansen",
            "email": "xen@xen.dk"
        }
    ],
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "require": {
        "php": "^7.3",
        "mnapoli/silly": "1.5.*",
        "mnapoli/silly-php-di": "^1.2",
        "symfony/yaml": "^3.4",
        "facebook/webdriver": "^1.6",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^3.4",
        "nesbot/carbon": "^2.26"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.4",
        "bossa/phpspec2-expect": "^2",
        "appocular/coding-standard": "^1.0",
        "friends-of-phpspec/phpspec-code-coverage": "^3"
    },
    "autoload": {
        "psr-4": {
            "Rorschach\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/appocular/coding-standard"
        }
    ]
}
